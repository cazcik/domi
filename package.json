{
  "name": "domi",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "CLOUDFLARE_ENV=development react-router dev",
    "preview": "CLOUDFLARE_ENV=development pnpm run build && vite preview",
    "start": "wrangler dev",
    "typecheck": "wrangler types && react-router typegen && tsc -b",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@remixicon/react": "^4.6.0",
    "clsx": "^2.1.1",
    "isbot": "^5.1.25",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.4.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^0.1.17",
    "@react-router/dev": "^7.4.0",
    "@tailwindcss/vite": "^4.0.17",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.17",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.5.0"
  },
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
